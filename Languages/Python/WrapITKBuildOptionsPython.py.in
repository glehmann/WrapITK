
import itkTemplate
import itkTypes

DIMS = [int(s) for s in "@WRAP_ITK_DIMS@".split(";")]
USIGN_INTS = [getattr(itkTypes, s) for s in "@WRAP_ITK_USIGN_INT@".split(";")]
SIGN_INTS = [getattr(itkTypes, s) for s in "@WRAP_ITK_SIGN_INT@".split(";")]
REALS = [getattr(itkTypes, s) for s in "@WRAP_ITK_REAL@".split(";")]

VECTOR_REALS = [itkTemplate.itkTemplate.__templates__[itkTemplate.normalizeName(s)] for s in "@WRAP_ITK_PYTHON_VECTOR_REAL@".split(";")]
COV_VECTOR_REALS = [itkTemplate.itkTemplate.__templates__[itkTemplate.normalizeName(s)] for s in "@WRAP_ITK_PYTHON_COV_VECTOR_REAL@".split(";")]
RGBS = [itkTemplate.itkTemplate.__templates__[itkTemplate.normalizeName(s)] for s in "@WRAP_ITK_PYTHON_RGB@".split(";")]
RGBAS = [itkTemplate.itkTemplate.__templates__[itkTemplate.normalizeName(s)] for s in "@WRAP_ITK_PYTHON_RGBA@".split(";")]
COMPLEX_REALS = [itkTemplate.itkTemplate.__templates__[itkTemplate.normalizeName(s)] for s in "@WRAP_ITK_PYTHON_COMPLEX_REAL@".split(";")]


INTS = SIGN_INTS + USIGN_INTS
SCALARS = INTS + REALS
VECTORS = VECTOR_REALS + COV_VECTOR_REALS
COLORS = RGBS + RGBAS
ALL_TYPES = COLORS + VECTORS + SCALARS + COMPLEX_REALS

del itkTemplate
del itkTypes
